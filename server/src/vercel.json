{
    "version":2,
    "builds":[
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "route":[
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "framework": {
        "node": "@vercel/node"
    },
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]

    
}